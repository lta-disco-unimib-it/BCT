# Begin Custom Macros
OutDir=.\Release
# End Custom Macros

ALL : "$(OUTDIR)\JClassStat.lib"


CLEAN :
	-@erase "$(INTDIR)\JavaHelpers.obj"
	-@erase "$(INTDIR)\JClassBuilder.obj"
	-@erase "$(INTDIR)\JClassFile.obj"
