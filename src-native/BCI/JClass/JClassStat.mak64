	"$(INTDIR)\JavaHelpers.obj" \
	"$(INTDIR)\JClassBuilder.obj" \
	"$(INTDIR)\JClassFile.obj" \
	"$(INTDIR)\JFileStream.obj" \
	"$(INTDIR)\JMemStream.obj" \
	"$(INTDIR)\JStream.obj"

"$(OUTDIR)\JClassStat.lib" : "$(OUTDIR)" $(DEF_FILE) $(LIB32_OBJS)
    $(LIB32) @<<
  $(LIB32_FLAGS) $(DEF_FLAGS) $(LIB32_OBJS)
<<

